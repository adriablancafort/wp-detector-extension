{
    "manifest_version": 3,
    "name": "WordPress Theme and Plugin Detector",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon-16.png",
            "48": "icon-48.png",
            "128": "icon-128.png"
        }
    },
    "background": {
        "service_worker": "sw.js"
    },
    "permissions": [
        "activeTab"
    ],
    "host_permissions": [
        "https://api.wp-detector.com/"
    ]
}